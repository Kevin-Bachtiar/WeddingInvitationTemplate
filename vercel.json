{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public",
          "buildCommand": "npm run build"
        }
      }
    ],
    "routes": [
      {
        "src": "/build/assets/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }